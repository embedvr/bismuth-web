{
	"name": "bismuth-web",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "pnpm run build && wrangler pages dev",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"deploy": "pnpm run build && wrangler pages deploy",
		"cf-typegen": "wrangler types && mv worker-configuration.d.ts src/",
		"db:migrate": "pnpm run db:migrate:local && pnpm run db:migrate:remote",
		"db:migrate:local": "wrangler d1 migrations apply bismuth --local",
		"db:migrate:remote": "wrangler d1 migrations apply bismuth --remote",
		"db:generate": "drizzle-kit generate",
		"db:studio": "drizzle-kit studio"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20240729.0",
		"@sveltejs/adapter-auto": "^3.2.2",
		"@sveltejs/adapter-cloudflare": "^4.7.0",
		"@sveltejs/kit": "^2.5.20",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@types/eslint": "^9.6.0",
		"autoprefixer": "^10.4.20",
		"drizzle-kit": "^0.22.8",
		"eslint": "^9.8.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.43.0",
		"globals": "^15.9.0",
		"postcss": "^8.4.40",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"svelte": "^4.2.18",
		"svelte-check": "^3.8.5",
		"tailwindcss": "^3.4.7",
		"typescript": "^5.5.4",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.3.5",
		"wrangler": "^3.68.0"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/geist-mono": "^5.0.3",
		"@fontsource/geist-sans": "^5.0.3",
		"@lucia-auth/adapter-drizzle": "^1.0.7",
		"@trpc/client": "^10.45.2",
		"@trpc/server": "^10.45.2",
		"@twurple/api": "^7.1.0",
		"@twurple/auth": "^7.1.0",
		"arctic": "^1.9.2",
		"bits-ui": "^0.21.13",
		"clsx": "^2.1.1",
		"cross-fetch": "^4.0.0",
		"date-fns": "^3.6.0",
		"drizzle-orm": "^0.31.4",
		"formsnap": "^1.0.1",
		"ky": "^1.5.0",
		"lucia": "^3.2.0",
		"lucide-svelte": "^0.420.0",
		"mode-watcher": "^0.4.1",
		"nanoid": "^5.0.7",
		"svelte-sonner": "^0.3.27",
		"sveltekit-superforms": "^2.16.1",
		"tailwind-merge": "^2.4.0",
		"tailwind-variants": "^0.2.1",
		"trpc-sveltekit": "^3.6.2",
		"zod": "^3.23.8"
	},
	"pnpm": {
		"overrides": {
			"@d-fischer/cross-fetch": "npm:cross-fetch@4.0.0"
		}
	}
}
